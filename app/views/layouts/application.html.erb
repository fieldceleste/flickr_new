<!DOCTYPE html>
<html>
  <head>
    <title>Find your Inspiration | Flickr</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

  </head>

  <body>
  <h1>flickr</h1>
  <div class="container">
    <nav class="navbar navbar-default">
      <div class="container-fluid">
        <div class="navbar-header">
          <h1 class="navbar-brand"></h1>
        </div>
        <ul class="nav navbar-nav">
          <li class="link"><a href="/">Home</a></li>
          <li class="link"><a href="/photos/new">Add a photo</a></li>
          <li class="link"><a href="/users/sign_in">Sign in</a></li>
          <li class="link"><a href="/users/sign_up">Sign up</a></li>
          <li class="link"><%= link_to "Sign Out", destroy_user_session_path, :method => 'delete'%></li>
          <li class="link"><a href="/photos">Photos</a></li>
        </ul>
      </div>
    </nav>
    
  </div>
  <%= flash[:alert] %>
  <%= flash[:notice] %>
    <%= yield %>
    
  </body>
</html>

